{
    "cheatSheet": [
        {
            "label": "Behat",
            "command": "php -d memory_limit=512M vendor/bin/behat"
        },
        {
            "label": "Behat progress",
            "command": "php -d memory_limit=512M vendor/bin/behat -f progress"
        },
        {
            "label": "PHP Stan",
            "command": "vendor/bin/phpstan analyse -c build/conf.d/phpstan.neon src --memory-limit 2G"
        },
        {
            "label": "PHP Fixer dry run",
            "command": "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist --dry-run --diff"
        },
        {
            "label": "PHP Fixer",
            "command": "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist"
        },
        {
            "label": "PMD",
            "command": "vendor/bin/phpmd src ansi build/conf.d/phpmd.xml"
        },
        {
            "label": "Open Api",
            "command": "bin/console alzura:open-api:create"
        }
    ],
